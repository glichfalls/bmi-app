<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.glichfalls.bmiapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloWorldApp">
        <activity android:name="com.glichfalls.bmiapp.activity.CreateUserActivity"></activity>
        <activity android:name="com.glichfalls.bmiapp.activity.SettingsActivity" />
        <activity android:name="com.glichfalls.bmiapp.activity.BMIListItemActivity" />
        <activity android:name="com.glichfalls.bmiapp.activity.BMIListActivity" />
        <activity android:name="com.glichfalls.bmiapp.activity.BmiDetailsActivity" />
        <activity android:name="com.glichfalls.bmiapp.activity.BMIActivity" />
        <activity android:name="com.glichfalls.bmiapp.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>